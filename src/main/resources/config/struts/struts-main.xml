<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="userMain" extends="main">
		<action name="userTunnelList" class="tunnelAction" method="tunnelList">
			<result name="success">user/tunnel/TunnelList.jsp</result>
		</action>
		<action name="userTunnelDetail" class="tunnelAction" method="tunnelUpdate" >
			<result name="success">user/tunnel/TunnelDetail.jsp</result>
		</action>
		<action name="userTunnelSectionList" class="tunnelSectionAction" method="tunnelSectionList">
			<result name="success">user/tunnelSection/TunnelSectionList.jsp</result>
		</action>
		<action name="userTunnelSectionDetail" class="tunnelSectionAction" method="tunnelSectionUpdate" >
			<result name="success">user/tunnelSection/TunnelSectionDetail.jsp</result>
		</action>
		<action name="userTunnelSectionState" class="userLiningRingConstructionAction" method="liningRingConstructionList">
			<result name="success">user/tunnelSection/TunnelSectionState.jsp</result>
		</action>
		<action name="userLiningRingConstructionDetail" class="userLiningRingConstructionAction" method="liningRingConstructionDetail">
			<result name="success">user/liningRingConstruction/LiningRingConstructionDetail.jsp</result>
		</action>
		<action name="userLiningRingConstructionList" class="liningRingConstructionAction" method="liningRingConstructionList">
			<result name="success">user/liningRingConstruction/LiningRingConstructionList.jsp</result>
		</action>
		<action name="userLiningRingDeformationQuery" class="liningRingDeformationAction" method="liningRingDeformationQuery">
			<result name="success">user/liningRingConstruction/LiningRingDeformation.jsp</result>
		</action>
		
		
	</package>	
</struts>
