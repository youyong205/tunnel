    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="ajax" extends="json-default">
		<action name="queryAllTunnelSections" class="tunnelSectionAction" method="queryAllTunnelSections">
			<result type="json"></result>
		</action>
		<action name="queryAllRectangleComponents" class="rectangleComponentAction" method="queryAllRectangleComponents">
			<result type="json"></result>
		</action>
		<action name="queryAllSaddleWeights" class="saddleWeightAction" method="queryAllSaddleWeights">
