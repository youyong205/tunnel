		</action>
		<action name="queryAllBrackets" class="bracketAction" method="queryAllBrackets">
			<result type="json"></result>
		</action>
		<action name="queryAllFlueSheets" class="flueSheetAction" method="queryAllFlueSheets">
			<result type="json"></result>
		</action>
		<action name="queryAllPlanks" class="plankAction" method="queryAllPlanks">
			<result type="json"></result>
		</action>
	</package>
